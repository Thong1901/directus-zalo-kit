		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^5.4.1",
		"@directus/extensions-sdk": "16.0.2",
		"@types/node": "^24.5.2",
		"eslint": "^9.36.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	}
}
